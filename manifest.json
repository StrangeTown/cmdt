{
  "manifest_version": 3,
  "name": "CMDT",
  "version": "1.0",
  "description": "Simple new tab extension",
  "author": "Xing",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "history",
    "downloads",
    "tabs",
    "downloads.open"
  ],
  "icons": {
    "16": "images/icon16.png",
    "128": "images/icon128.png"
  }
}
